# $ docker run -it --name ar_display -v $(pwd):/app -v $(pwd)/variables:/data -v $(pwd)/../htdocs:/htdocs -v $(pwd)/templates:/templates  -p 5050:5050 -p 5051:5051 -d ar_display_img:latest

